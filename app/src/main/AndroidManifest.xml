<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.roblav96.highdpinavbarbuttonsfixed">

    <application
        android:allowBackup="false"
        android:label="High Dpi Navbar Buttons Fixed"
        android:icon="@mipmap/ic_launcher"
        android:roundIcon="@mipmap/ic_launcher_round">
        <meta-data
            android:name="xposedmodule"
            android:value="true" />
        <meta-data
            android:name="xposedminversion"
            android:value="82" />
        <meta-data
            android:name="xposeddescription"
            android:value="Fixes navbar buttons orientation when using an extremely high DPI setting, example: 'adb shell wm density 640'. Originally forked from https://github.com/tfae/Oreo-Lock-Screen-Album-Art-Remover and used as a boilerplate." />
    </application>
</manifest>
